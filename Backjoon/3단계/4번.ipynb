   "source": [
    "total = int(input())\n",
    "product = int(input())\n",
    "\n",
    "for _ in range(product):\n",
    "    price, count = map(int, input().split())\n",
    "    total -= price * count\n",
    "\n",
    "if total == 0:\n",
    "    print('Yes')\n",
    "else:\n",
    "    print('No')"
   ]
