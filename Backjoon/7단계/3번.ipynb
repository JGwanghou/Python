   "source": [
    "input_num = int(input())\n",
    "\n",
    "line = 0\n",
    "max_num = 0\n",
    "while input_num > max_num:\n",
    "    line += 1\n",
    "    max_num += line\n",
    "gap = max_num - input_num\n",
    "\n",
    "if line %2 == 0:\n",
    "    top = line - gap\n",
    "    under = gap + 1\n",
    "else:\n",
    "    top = gap + 1\n",
    "    under = line - gap\n",
    "print()"
   ]
  }
